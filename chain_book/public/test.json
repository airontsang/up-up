                        "format": "string"
                    }
                ],
                "tags": [
                    "User"
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "title": "Signin_Info",
                            "type": "object",
                            "properties": {
                                "msg": {
                                    "type": "string"
                                },
                                "loginId": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Error response",
                        "schema": {
                            "title": "reg_Error_Info",
                            "type": "string"
                        }
                    }
                }
            }
        },
