{
    "swagger": "2.0",
    "info": {
        "version": "1.0.0",
        "title": "毕业设计后台API"
    },
    "paths": {
        "/User/Login": {
            "get": {
                "description": "Login \n",
                "parameters": [
                    {
                        "name": "loginId",
                        "in": "query",
                        "description": "loginId",
                        "required": true,
                        "type": "string",
                        "format": "string"
                    },
                    {
                        "name": "passWord",
                        "in": "query",
                        "description": "password",
                        "required": true,
                        "type": "string",
                        "format": "string"
                    }
                ],
                "tags": [
                    "User"
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "title": "Login_Info",
                            "type": "object",
                            "properties": {
                                "loginId": {
                                    "type": "string"
                                },
                                "token": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Error response",
                        "schema": {
                            "title": "Login_Error_Info",
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/User/reg": {
            "post": {
                "description": "Posts a `User` object fro Signin\n",
                "parameters": [
                    {
                        "name": "loginId",
                        "in": "query",
